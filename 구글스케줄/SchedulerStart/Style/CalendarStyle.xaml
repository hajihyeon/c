                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="WeekHeaderDayStyle" TargetType="{x:Type Label}">
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Foreground" Value="Gray" />
        <Setter Property="FontSize" Value="45" />
    </Style>

    <Style x:Key="WeekHeaderToDayStyle" TargetType="{x:Type Label}">
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontSize" Value="55" />
    </Style>

    <Style x:Key="WeekHeaderTimeStyle" TargetType="{x:Type Label}">
        <Setter Property="FontSize" Value="30" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Center" />
    </Style>

    
    
    <!-- 여기서부터는 유저가 지정할 색-->
    <Style x:Key="WeekBorderuser" TargetType="{x:Type Border}">
        <Setter Property="BorderBrush" Value="White" />
        <Setter Property="BorderThickness" Value="3 0 0 0" />
    </Style>

    
